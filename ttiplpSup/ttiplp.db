record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
    field(VAL, "$(RECSIM=0)")
    field(PINI, "NO")
}

record(bo, "$(P)DISABLE")
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "$(DISABLE=0)")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}
# Device identity - waveform as the identity is too long for a string.
record(waveform, "$(P,undefined)IDENT")
    {
    field(DESC, "Identity")
    field(DTYP, "stream")
    field(PINI, "YES")
    field(INP,  "@ttiplp.proto getIDN $(PORT,undefined)")
    field(SCAN, "10 second")
    field(NELM, "128")
    field(FTVL, "CHAR")
    info(INTEREST, "LOW")
}

